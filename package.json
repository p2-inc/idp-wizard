{
  "name": "idp-wizard",
  "version": "0.0.2",
  "description": "Frontend for PhaseTwo Identity Provider Application",
  "repository": "https://github.com/p2-inc/idp-wizard",
  "homepage": "http://app.phasetwo.io/",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dr:surge": "node dr-surge.js",
    "build": "yarn clean && webpack --config webpack.prod.js && yarn dr:surge",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --color --progress --config webpack.dev.js",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "dependencies": {
    "@keycloak/keycloak-admin-client": "^15.0.2",
    "@patternfly/patternfly": "^4.185.1",
    "@patternfly/react-core": "^4.202.16",
    "@patternfly/react-icons": "^4.53.16",
    "@patternfly/react-styles": "^4.52.16",
    "@patternfly/react-table": "^4.71.16",
    "@react-keycloak/web": "^3.4.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@storybook/builder-webpack5": "^6.2.9",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "formik": "^2.2.9",
    "keycloak-js": "^15.0.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.30",
    "nanoid-dictionary": "^4.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.2.1",
    "react-use": "^17.3.1",
    "redux-persist": "^6.0.0",
    "sirv-cli": "^1.0.12",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/victory": "^33.1.4",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "astroturf": "^1.0.0-beta.27",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "dotenv-webpack": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.28.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "imagemin": "^8.0.0",
    "jest": "^27.0.4",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss": "^8.3.1",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.3.1",
